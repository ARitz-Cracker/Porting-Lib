    },
    "modmenu": {
      "badges": [ "library" ],
      "parent": {
        "id": "porting_lib",
        "name": "Porting Lib",
        "description": "A collection of utilities for porting mods from Forge to Fabric.",
        "icon": "assets/porting_lib/icon.png",
        "badges": [ "library" ]
      }
    }
  }
