{
  "schemaVersion": 1,
  "id": "porting_lib_model_loader",
  "version": "${version}",
  "name": "Porting Lib Model Loader",
  "description": "Adds the framework for loading more types of models.",
  "entrypoints": {
    "client": [
      "io.github.fabricators_of_create.porting_lib.model_loader.client.PortingLibModelsClient"
    ]
  },
  "mixins": [
    "porting_lib_model_loader.mixins.json"
  ],
  "accessWidener": "porting_lib-model.accesswidener",
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_783": ["io/github/fabricators_of_create/porting_lib/model_loader/extensions/BlockElementFaceExtensions"],
      "net/minecraft/class_793": ["io/github/fabricators_of_create/porting_lib/model_loader/extensions/BlockModelExtensions"],
      "net/minecraft/class_4588": ["io/github/fabricators_of_create/porting_lib/model_loader/extensions/VertexConsumerExtension"]
    }
  }
}
