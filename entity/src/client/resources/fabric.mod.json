{
  "schemaVersion": 1,
  "id": "porting_lib-client-entity",
  "version": "${version}",
  "name": "Porting Lib",
  "description": "A collection of utilities for porting mods from Forge to Fabric.",
  "authors": [
    "The Create Fabric Team"
  ],
  "contact": {
    "sources": "https://github.com/Fabricators-of-Create/Porting-Lib"
  },
  "license": "LGPL",
  "icon": "assets/porting_lib/icon.png",
  "environment": "*",
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": ">=${fabric_version}",
    "minecraft": ">=${minecraft_version}",
    "java": ">=${java_version}"
  },
  "entrypoints": {
    "main": [
      "io.github.fabricators_of_create.porting_lib.entity.PortingLibEntity"
    ]
  },
  "custom": {
    "modmenu": {
      "badges": [ "library" ]
    }
  }
}
