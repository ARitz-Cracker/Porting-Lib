{
  "schemaVersion": 1,
  "id": "porting_lib",
  "version": "${version}",
  "name": "Porting Lib",
  "description": "A collection of utilities for porting mods from Forge to Fabric.",
  "authors": [
    "The Create Fabric Team"
  ],
  "contact": {
    "sources": "https://github.com/Fabricators-of-Create/Porting-Lib"
  },
  "license": "LGPL",
  "icon": "assets/porting_lib/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.fabricators_of_create.porting_lib.PortingLib"
    ],
    "client": [
      "io.github.fabricators_of_create.porting_lib.PortingLibClient"
    ],
    "mm:early_risers": [
      "io.github.fabricators_of_create.porting_lib.PortingLibER"
    ],
    "preLaunch": [
      "io.github.fabricators_of_create.porting_lib.PortingLibPreLaunch"
    ],
    "fabric-datagen": [
      "io.github.fabricators_of_create.porting_lib.PortingLibData"
    ]
  },
  "mixins": [
    "porting_lib.mixins.client.json",
    "porting_lib.mixins.common.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": ">=${fabric_version}",
    "minecraft": ">=${minecraft_version}",
    "java": ">=${java_version}"
  },
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_3611": ["io/github/fabricators_of_create/porting_lib/extensions/FluidExtensions"]
    },
    "modmenu": {
      "badges": [ "library" ]
    }
  }
}
