{
  "schemaVersion": 1,
  "id": "porting_lib-tags",
  "version": "${version}",
  "name": "Porting Lib Tags",
  "description": "A collection of utilities for porting mods from Forge to Fabric.",
  "authors": [
    "The Create Fabric Team"
  ],
  "contact": {
    "sources": "https://github.com/Fabricators-of-Create/Porting-Lib"
  },
  "license": "LGPL",
  "icon": "assets/porting_lib/icon.png",
  "environment": "*",
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": ">=${fabric_version}",
    "minecraft": ">=${minecraft_version}",
    "java": ">=${java_version}"
  },
  "provides": [
    "forge_tags"
  ],
  "entrypoints": {
    "main": [
      "me.alphamode.forgetags.Tags"
    ]
  },
  "mixins": [
    "porting_lib-tags.mixins.json"
  ],
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_1767": ["me/alphamode/forgetags/extensions/DyeExtension"]
    },
    "modmenu": {
      "badges": [ "library" ],
      "parent": {
        "id": "porting_lib",
        "name": "Porting Lib",
        "description": "A collection of utilities for porting mods from Forge to Fabric.",
        "icon": "assets/porting_lib/icon.png",
        "badges": [ "library" ]
      }
    }
  }
}
